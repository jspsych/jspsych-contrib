    "build:watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jspsych/jspsych-contrib/",
    "directory": "packages/plugin-spatial-nback"
  },
  "keywords": [
    "jsPsych"
  ],
  "author": {
